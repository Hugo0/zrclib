{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "build/**",
        "cache/**",
        "artifacts/**",
        "typechain-types/**",
        "contracts/generated/**",
        "pot/**"
      ]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
